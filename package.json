{
  "name": "erp-seed",
  "version": "0.0.1",
  "description": "A Node web app seed with Express, Redis, Sequelize, and Webpack.",
  "main": "server.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --env=production",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "forever": "forever start ./bin/www",
    "lint:server": "./node_modules/.bin/eslint ./server/**/*.js",
    "lint:client": "./node_modules/.bin/eslint ./client/**/*.js",
    "redis": "redis-server",
    "server": "node ./bin/www",
    "server:dev": "nodemon ./bin/www",
    "client:dev": "./node_modules/.bin/webpack --progress --profile --colors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Grant J. Schulte <schulte.grant@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "bcrypt": "1.0.3",
    "body-parser": "1.18.2",
    "connect-redis": "3.3.2",
    "cookie-parser": "1.4.3",
    "copy-webpack-plugin": "4.2.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "express": "4.16.2",
    "express-session": "1.15.6",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "forever": "0.15.3",
    "imagemin-webpack-plugin": "1.5.2",
    "morgan": "1.9.0",
    "node-sass": "4.7.1",
    "pg": "7.4.0",
    "pg-hstore": "2.3.2",
    "postcss-clean": "1.1.0",
    "postcss-cli": "4.1.1",
    "postcss-loader": "2.0.8",
    "postcss-scss": "1.0.2",
    "pug": "2.0.0-rc.4",
    "redis": "2.8.0",
    "sass-loader": "6.0.6",
    "sequelize": "4.22.6",
    "style-loader": "0.19.0",
    "uglifyjs-webpack-plugin": "1.0.1",
    "url-loader": "0.6.2",
    "webpack": "3.8.1"
  },
  "devDependencies": {
    "connect-livereload": "0.6.0",
    "eslint": "4.11.0"
  }
}
